<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="msk.GuiApplication">
    <children>
        <MenuBar layoutX="33.0" layoutY="73.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <menus>
                <Menu mnemonicParsing="false" text="Symulacja">
                    <items>
                        <MenuItem fx:id="startButton" mnemonicParsing="false" onAction="#startSimBtn" text="Rozpocznij" />
                        <MenuItem fx:id="endButton" disable="true" mnemonicParsing="false" onAction="#stopSimBtn" text="Zatrzymaj" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>

        <Label layoutX="14.0" layoutY="472.0" text="simTime:">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Label fx:id="simTime" layoutX="99.0" layoutY="473.0" text="Label">
            <font>
                <Font name="System Bold" size="20.0" />
            </font>
        </Label>
    </children>

</AnchorPane>
